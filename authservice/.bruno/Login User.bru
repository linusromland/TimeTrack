meta {
  name: Login User
  type: http
  seq: 3
}

post {
  url: {{URL}}/auth/login
  body: json
  auth: inherit
}

body:json {
  {
    "email": "user@example.com",
    "password": "examplePassword"
  }
}

script:post-response {
  function onResponse(response) {
    const body = response.getBody()
    bru.setEnvVar('jwt_token', body.token)
  }
}
